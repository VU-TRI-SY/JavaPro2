package tut1;

public class Ex3_1 {
	int die1, die2;
	int countRolls = 0;
	while(true) {
		die1 = (int)(Math.random()*6) + 1;  
        die2 = (int)(Math.random()*6) + 1;
        if(die1 != 1 && die2 != 1) count++;
        else break;
	}
	 System.out.println("It took " + count + " rolls to get snake eyes.");
}
